<UserControl x:Class="HighlightPlayer.Controls.HighlightMediaElement"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HighlightPlayer.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
             x:Name="control">

                  <!--LoadedBehavior="{Binding ElementName=control,Path=State,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"-->
    <MediaElement Name="player" ScrubbingEnabled="True"
                  LoadedBehavior="Manual"
                  MediaOpened="Player_MediaOpened"
                  MediaEnded="Player_MediaEnded"/>
</UserControl>
